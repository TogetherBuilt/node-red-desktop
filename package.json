{
  "name": "node-red-installer",
  "version": "2.2.0",
  "main": "main.js",
  "build": {
    "appId": "com.electron.node-red",
    "productName": "Node-RED",
    "asar": false,
    "files": [
      "**/*",
      {
        "from": "node_modules/@node-red",
        "to": "node_modules/@node-red"
      },
      {
        "from": "build",
        "to": "build"
      }
    ],
    "win": {
      "target": [
        {
          "target": "msi"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg"
        }
      ]
    },
    "linux": {
      "category": "Development",
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64",
            "armv7l"
          ]
        },
        {
          "target": "rpm"
        }
      ],
      "publish": []
    }
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "dependencies": {
    "electron-log": "4.4.6",
    "node-red": "2.2.0"
  },
  "devDependencies": {
    "electron": "17.0.0",
    "electron-builder": "22.14.2"
  },
  "description": "Node-RED installer",
  "keywords": [
    "node-red",
    "installer",
    "standalone",
    "electron"
  ],
  "license": "Apache-2.0",
  "homepage": "https://nodered.org",
  "contributor": [
    "Kazuhito Yokoi",
    "Dave Conway-Jones",
    "Nick O'Leary"
  ],
  "author": "Node-RED <team@nodered.org>"
}
